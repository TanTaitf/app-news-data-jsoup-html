        <activity android:name=".ModelLoad.ListViewNewsActivity"></activity>
        <activity android:name=".ModelLoad.WebviewNewsActivity"></activity>
        <activity android:name=".WellcomHome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>